package week_two

object rationals {
  val r = new Rational(2, 3)                      //> r  : week_two#35.Rational#687269 = 2/3
}

class Rational(x: Int, y: Int){
	def numer=x
	def denom=y
	
	def add(that: Rational)=
		new Rational(
			numer*that.denom + that.numer * denom,
			denom * that.denom
		)
	//override def toString=numer+"/"+denom
}
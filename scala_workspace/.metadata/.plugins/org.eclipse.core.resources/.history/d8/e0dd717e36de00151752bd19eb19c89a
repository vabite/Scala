package patmat

import Huffman.decodedSecret

object prove {
	val n = 2                                 //> n  : Int = 3
  1 to n map (i => 1 to i map (j => (j, i)))      //> res0: scala.collection.immutable.IndexedSeq[scala.collection.immutable.Index
                                                  //| edSeq[(Int, Int)]] = Vector(Vector((1,1)), Vector((1,2), (2,2)), Vector((1,3
                                                  //| ), (2,3), (3,3)))
    
}
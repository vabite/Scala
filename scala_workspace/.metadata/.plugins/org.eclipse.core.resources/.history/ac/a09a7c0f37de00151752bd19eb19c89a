package patmat

import Huffman.decodedSecret

object prove {
	val n = 2                                 //> n  : Int = 2
  (1 to n map (i => 1 to i map (j => (j, i)))) flatMap(x=>x)
                                                  //> res0: scala.collection.immutable.IndexedSeq[(Int, Int)] = Vector((1,1), (1,2
                                                  //| ), (2,2))
    
}